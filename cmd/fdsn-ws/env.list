DB_HOST=localhost
DB_USER=fdsn_r
DB_PASSWD=test
DB_NAME=fdsn
DB_SSLMODE=disable
DB_CONN_TIMEOUT=5
DB_MAX_IDLE_CONNS=20
DB_MAX_OPEN_CONNS=30

# The station service is proxied.
FDSN_STATION=service.geonet.org.nz

# The AWS S3 bucket that holds the miniseed files served by dataselect.
# AWS_ACCESS_KEY_ID and AWS_SECRET_ACCESS_KEY (or other valid AWS credential method)
# must be properly set to access this bucket.
AWS_REGION=ap-southeast-2
S3_BUCKET=fdsn-data.geonet.org.nz

